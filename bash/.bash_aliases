# 代替键盘上难以触及的 ~ 键
alias home="cd ~"

# 回退多级目录
alias bk1="cd .."  
alias bk2="cd ../.."  
alias bk3="cd ../../.."  
alias bk4="cd ../../../.."  
alias bk5="cd ../../../../.."

# 其他常用别名
alias ll='ls -l'
alias gs='git status'

# 到达指定（常用）的目录
# 代码（工作）目录
alias toroot='cd /www/html/'

# nginx 配置目录
alias ng="cd /www/server/panel/vhost/nginx"
